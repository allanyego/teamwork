{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "extends": ["airbnb"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "no-console": 0
    },
    "plugins": [
      "jasmine"
    ]
}
